fun main (){
    
var myArray = arrayOf(65,3,8,886,4521)
var Out = maxValue(myArray)
println (Out)
}

// Функция находит самое большое значение в массиве

fun maxValue(args: Array<Int>): Int {
 var max = args[0]
 var x = 1
 while (x < args.size) {
 var item = args[x]
 max = if (max >= item) max else item
 x = x + 1
 }
 return max
 }
